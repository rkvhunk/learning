   "source": [
    "l = [3,8,5,7,4,6,1,2,-4,0,4.4]\n",
    "final_op = []\n",
    "for i in l:\n",
    "    if i%2==0 and i>0:\n",
    "        final_op.append(i)\n",
    "result = sorted(final_op)\n",
    "print(result)\n",
    "result_odd = []\n",
    "for i in result:\n",
    "    result_odd.append(i-1)\n",
    "print(result_odd)"
   ]

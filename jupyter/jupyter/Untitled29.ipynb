   "source": [
    "def dec(f):\n",
    "    def inner():\n",
    "        print(\"inside decorator....\")\n",
    "        f()\n",
    "        print(\"!!!!!!!!!!!\")\n",
    "    return inner\n",
    "\n",
    "@dec\n",
    "def fun():\n",
    "    print(\"outside descorator...\")\n",
    "\n",
    "fun()"
   ]
